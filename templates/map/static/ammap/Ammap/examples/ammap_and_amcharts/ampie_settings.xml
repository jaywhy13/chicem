<settings> 
                                                               
  <pie>
    <x>250</x>
    <y>250</y>
    <radius>90</radius>                        
    <alpha>80</alpha>                        
  </pie>
  
  <animation>
    <start_time>2</start_time>                 
    <start_effect>strong</start_effect>        
    <pull_out_time>1.5</pull_out_time>         
    <pull_out_effect>Bounce</pull_out_effect>  
  </animation>
  
  <data_labels>
    <show>
       <![CDATA[{title}: {percents}%]]>        
    </show>
  </data_labels>

  <group>
    <percent>3</percent>                       
  </group>

    
  <legend>                                     
    <enabled>false</enabled>                   
  </legend>  
  

</settings>
